# GitHub Container Registry
GITHUB_REPOSITORY_OWNER=your-github-username

# Database Configuration
POSTGRES_USER=booksy_admin
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=booksy_production
DB_CONNECTION_STRING=Host=postgres;Port=5432;Database=booksy_production;Username=booksy_admin;Password=your-secure-password-here;Include Error Detail=true

# Redis Configuration
REDIS_PASSWORD=your-redis-password-here
REDIS_CONNECTION_STRING=redis:6379,password=your-redis-password-here

# RabbitMQ Configuration
RABBITMQ_USER=booksy_admin
RABBITMQ_PASSWORD=your-rabbitmq-password-here
RABBITMQ_CONNECTION_STRING=amqp://booksy_admin:your-rabbitmq-password-here@rabbitmq:5672

# Seq Configuration
SEQ_ADMIN_USER=admin
SEQ_ADMIN_PASSWORD=your-seq-password-here
SEQ_SERVER_URL=http://seq:5341

# Service URLs (Internal)
USER_MANAGEMENT_URL=http://usermanagement-api:80
SERVICE_CATALOG_URL=http://servicecatalog-api:80

# Frontend Configuration
API_BASE_URL=http://your-server-ip:5000

# pgAdmin Configuration
PGADMIN_EMAIL=admin@booksy.local
PGADMIN_PASSWORD=your-pgadmin-password-here
